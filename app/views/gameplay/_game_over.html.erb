<script>
  function endText() {
    var color = game.turn()
    if (color === "w") {
      var text = ("Your quest ends here")
    } else if (game.in_stalemate()) {
      var text = ("Your quest ends here")
    } else {
      var text = ("You Won!!")
    }
    if (game.game_over()) {
      alert(text);
    }
  }
</script>
<script>
  function winner () {
    if (game.in_checkmate() && game.turn() === "b") {
      return 1
    } else if (game.in_checkmate() || game.in_stalemate()) {
      return 2
    } else {
      return 0
    }
  }
</script>
